        <mj-section mj-class="section">
            <mj-column>
                <mj-image  href="https://t.me/wbdaychat" src="https://i.ibb.co/gz2gK9s/telegram-icon.png" alt="WBDay Telegram Icon" width="25px" padding-bottom="21px"/>

                <mj-text font-weight="500"
