                    <mj-column border-radius="20px" background-color="#fff">
                        <mj-text
                                font-weight="400"
                                font-size="24px"
                                color="#000000"
                                line-height="24px"
                                padding-top="20px"
                        >Offline</mj-text
                        >
                        <mj-text
                                font-weight="400"
                                font-size="14px"
                                color="#000000"
                                line-height="16px"
