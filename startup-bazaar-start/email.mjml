                        <mj-divider border-color="#D7D7D7" width="484px" border-width="1px"/>
                        <mj-text
                                font-weight="400"
                                font-size="24px"
                                color="#000000"
                                line-height="26px"
                                padding="15px 24px 15px 24px"
