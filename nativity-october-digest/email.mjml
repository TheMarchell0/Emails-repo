<mjml>
    <mj-head>
        <mj-title></mj-title>
        <mj-attributes>
            <mj-all font-family="Raleway, Arial, Helvetica, sans-serif" />
            <mj-class name="body" width="600px" background-color="#000000" />
            <mj-class name="section" padding="0" background-color="#FFFFFF"/>
            <mj-class name="hero-image" padding="0" width="600px" />
        </mj-attributes>
        <mj-style>
            .block-shadow {
                box-shadow: 8.11111px 1.62222px 40.55555px 0px rgba(0, 0, 0, 0.25);!important;
