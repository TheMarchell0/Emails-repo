<mjml>
    <mj-head>
        <mj-title></mj-title>
        <mj-attributes>
            <mj-all font-family="Helvetica, Arial, sans-serif" />
            <mj-class name="body" width="600px" background-color="#FFFFFF" />
            <mj-class name="section" padding="0" background-color="#FFFFFF" />
            <mj-class
                    name="titleText"
                    padding="0px"
                    font-weight="700"
                    font-size="14px"
                    line-height="16px"
                    color="#000000"
            />
            <mj-class
                    name="contentText"
                    padding="0px"
                    font-weight="400"
                    font-size="12px"
                    line-height="14px"
                    color="#000000"
            />
            <mj-class name="hero-image" padding="0" width="600px" />
            <mj-class
                    name="button"
                    border-radius="0"
                    padding="0"
                    inner-padding="10px"
                    font-weight="700"
                    font-size="10px"
                    line-height="11px"
                    color="#FFFFFF"
                    background-color="#649C47"
                    width="128px"
                    text-transform="uppercase"
            />
            <mj-class name="footer" padding="30px 0 0" />
            <mj-class
                    name="footer__text"
                    font-weight="300"
                    font-size="10px"
                    line-height="11px"
                    color="rgba(255, 255, 255, 0.6)"
            />
        </mj-attributes>
        <mj-style> </mj-style>
    </mj-head>
    <mj-body mj-class="body">
        <mj-section mj-class="section">
            <mj-column>
                <mj-image
                        mj-class="hero-image"
                        src="https://i.ibb.co/H7rPZrW/hero-img.png"
