<mjml>
  <mj-head>
    <mj-title></mj-title>
    <mj-attributes>
      <mj-all font-family="Roboto Flex, Helvetica, Arial, sans-serif" />
      <mj-class name="body" width="600px" background-color="#FFFFFF" />
      <mj-class name="section" padding="0" background-color="#18191A" />
      <mj-class
        name="titleText"
        padding="0px 40px 0px 40px"
        font-weight="900"
        font-size="24px"
        line-height="36px"
        color="#FFFFFF"
        font-style="italic"
        text-transform="uppercase"
      />
      <mj-class
        name="contentText"
        padding="0px 40px 0px 40px"
        font-weight="500"
        font-size="15px"
        line-height="1.2"
        color="#FFFFFF"
      />
      <mj-class name="hero-image" padding="0" width="640px" />
      <mj-class
        name="button"
        border-radius="0"
        padding="0"
        inner-padding="6px 20px"
        font-style="italic"
        font-weight="900"
        font-size="24px"
        line-height="36px"
        text-transform="uppercase"
      />
      <mj-class name="footer" padding="36px 0 0" />
      <mj-class
        name="footer__text"
        padding="0 53px 34px 47px"
        font-weight="400"
        font-size="14px"
        line-height="16px"
        color="rgba(255, 255, 255, 0.5)"
      />
